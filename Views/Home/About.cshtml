@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<p>Use this area to provide additional information.</p>
<div class="row">
    <div class="col-md-4">
        @using (Html.BeginForm("About", "Home", FormMethod.Post))
        {
            <div class="form-group">

                @Html.Label("URL")
                @Html.TextBox("UrlData")
            </div>
            <div class="form-group">

                @Html.Label("Token")
                @Html.TextBox("TokenData")
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        }
    </div>
</div>
<h2>Output Responce</h2>
<div class="row">
    <ul class="list-group list-group-flush pmd-card-list ">
        @foreach (var item in ViewBag.Responces)
        {
        <li class="list-group-item h-100">
            <div class="col-md-12 ">
                <span>@item</span>
            </div>
            <div class="col">
                <hr>
            </div>
        </li>            
            
        }
    </ul>
</div>
